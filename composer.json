{
  "name": "lawoo-io/lawoo",
  "type": "library",
  "license": "BUSL-1.1",
  "description": "Modular Framework for Laravel Projects",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "Modules\\": "modules/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Modules\\Core\\Providers\\CoreServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php artisan package:discover"
    ]
  }
}
