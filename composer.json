{
    "name": "lawoo-io/lawoo",
    "type": "library",
    "license": "BUSL-1.1",
    "description": "Modular Framework for Laravel Projects",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^12.0",
        "livewire/livewire": "^3.0",
        "livewire/flux-pro": "^2.4",
        "symfony/dom-crawler": "^7.2",
        "symfony/yaml": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Modules\\": "modules/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Core\\Providers\\CoreServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": []
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
